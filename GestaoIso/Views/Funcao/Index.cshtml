@model IEnumerable<GestaoIso.Data.Funcao>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cargo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Experiencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DominioEducacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Treinamento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CriacaoResp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CriacaoData)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RevisaoResp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RevisaoData)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AprovacaoStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AprovacaoResp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AprovacaoData)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProdutoComentario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cargo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Experiencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DominioEducacao.DominioId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Treinamento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CriacaoResp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CriacaoData)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RevisaoResp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RevisaoData)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AprovacaoStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AprovacaoResp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AprovacaoData)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProdutoComentario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FuncaoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FuncaoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FuncaoId">Delete</a> |
                <a asp-action="Approve" asp-route-id="@item.FuncaoId">Approve</a>
            </td>
        </tr>
}
    </tbody>
</table>
